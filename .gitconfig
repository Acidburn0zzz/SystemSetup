[user]
	name = AJ Minich
	email = aj.minich@binatechnologies.com
[color]
	ui = auto
[branch]
	autosetupmerge = true
[push]
	default = current
[color "branch"]
	current = yellow bold
	remote = cyan bold
[color "diff"]
	new = yellow bold
	old = red bold
	frag = white bold
	meta = cyan bold
	commit = white bold
[color "status"]
	added = yellow bold
	changed = cyan bold
	untracked = red bold
[github]
	user = ajminich
[core]
	editor = vim
[credential]
    helper = cache --timeout=6400
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
